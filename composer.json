{
    "name": "greensight/test-factories",
    "description": "",
    "keywords": [
        "laravel",
        "factory"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ilya Nekrasov",
            "email": "nekrasov@greensight.ru"
        }
    ],
    "require": {
        "php": "^8.0",
        "fakerphp/faker": "^1.10",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.0",
        "php-parallel-lint/php-var-dump-check": "^0.5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Greensight\\TestFactories\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Greensight\\TestFactories\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": "php-cs-fixer fix --config .php_cs",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
